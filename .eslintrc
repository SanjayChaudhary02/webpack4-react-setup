{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "rules": {
    "quotes": [2, "single"],
    "semi": [2, "never"],
    "require-await": 2,
    "no-unused-vars": 2,
    "no-undef": 2,
    "for-direction": 2,
    "no-console": 1,
    "no-debugger": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-obj-calls": 2,
    "no-prototype-builtins": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-template-curly-in-string": 2,
    "no-unexpected-multiline": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "use-isnan": 2,
    "valid-jsdoc": 2,
    "valid-typeof": 2
  },
  "extends": "eslint:recommended"
}
